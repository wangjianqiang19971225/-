.top {
  padding: 0 20rpx;

  position: relative;
}

.show {
  display: block !important;
}

.hidden {
  display: none !important;
}


button::after {
  border: none;
}

button {
  background-color: #fff;
}

button {
  border-radius: 0;
}

.serch-button button {
  font-size: 24rpx;
  color: #fff;
  height: 70rpx;
  width: 126rpx;
  border-radius: 70rpx;
  background-color: #548cea;
  line-height: 70rpx;
  position: absolute;
  top: 0;
  right: 20rpx;
}

.input {
  height: 64rpx;
  width: 690rpx;
  border: 1rpx solid #d5d5d5;
  border-radius: 70rpx;
  font-size: 26rpx;
  line-height: 64rpx;
  margin-left: 30rpx;
  text-align: center;
}

.history {
  font-size: 30rpx;
  margin-top: 70rpx;
}

.history-a {
  padding: 0 30rpx;
}

.history image {
  height: 26rpx;
  width: 22rpx;
  float: right;
}

.history-top {
  padding: 0 32rpx;
  margin-top: 40rpx;
}

.history-top text {
  padding: 0 38rpx;
  background-color: #f4f4f4;
  height: 48rpx;
  line-height: 48rpx;
  font-size: 24rpx;
  border-radius: 48rpx;
  margin-right: 40rpx;
  margin-bottom: 40rpx;
  float: left;
  color: #666666;
}

.result {
  margin-top: 20rpx;
}

.result-top {
  height: 60rpx;
  background-color: #dadada;
  font-size: 26rpx;
  line-height: 60rpx;
  padding-left: 20rpx;
}

.result-top text {
  color: #3876e0;
}

/* 商家 */
.merchant {
  padding: 20rpx;
  margin: 0 20rpx;
  border: 1rpx solid #eeeeee;
  border-radius: 10rpx;
  margin-top: 10rpx;
  position: relative;
}

.merchant-top {
  padding-bottom: 20rpx;
  border-bottom: 1rpx solid #eeeeee;
}

.merchant-img {
  height: 120rpx;
  width: 120rpx;
  border: 1rpx solid #eeeeee;
  border-radius: 10rpx;
}

.merchant-name {
  font-size: 24rpx;
  font-weight: 700;
  position: absolute;
  top: 28rpx;
  left: 156rpx;
}

.merchant-address {
  font-size: 20rpx;
  color: #999999;
  position: absolute;
  top: 72rpx;
  left: 156rpx;
}

.merchant-distance-img {
  height: 22rpx;
  width: 16rpx;
  position: absolute;
  top: 117rpx;
  left: 156rpx;
}

.merchant-distance {
  font-size: 20rpx;
  color: #666666;
  position: absolute;
  top: 114rpx;
  left: 180rpx;
}

.merchant-right-img {
  height: 16rpx;
  width: 28rpx;
  position: absolute;
  top: 72rpx;
  right: 30rpx;
  transform: rotate(-90deg);
}

.merchant-list {
  margin-left: 136rpx;
  margin-top: 20rpx;
  overflow: hidden;
}

.merchant-item {
  float: left;
  height: 220rpx;
  width: 126rpx;
  margin-right: 7rpx;

}

.merchant-item image {
  height: 126rpx;
  width: 126rpx;
  border: 1rpx solid #eeeeee;
}

.merchant-item text {
  font-size: 24rpx;
  height: 92rpx;
  text-align: center;
  margin-top: 15rpx;
  display: block;
}

/* 商品 */
.merchandise {
  padding: 30rpx;
  /* background-color: red; */
  height: 190rpx;
  width: 690rpx;
  border-bottom: 2rpx solid #fafafb;
}

.left {
  float: left;
}

.right {
  float: left;
  margin-left: 30rpx;
  height: 190rpx;
  width: 504rpx;
  position: relative;
}

.merchandise-img {
  height: 190rpx;
  width: 156rpx;
  border-radius: 10rpx;
}

.merchandise-name {
  font-size: 30rpx;
}

.star image {
  height: 22rpx;
  width: 22rpx;
  margin-right: 10rpx;
}

.merchandise-time {
  font-size: 24rpx;
  color: #999999;
  margin-top: 10rpx;
}

.merchandise-position {
  overflow: hidden;
  margin-top: 10rpx;
}

.position {
  height: 22rpx;
  width: 22rpx;
  float: left;
  margin-top: 5rpx;
}

.position-txt {
  font-size: 22rpx;
  color: #999999;
  float: left;
  margin-left: 10rpx;
}

.distance {
  float: right;
  font-size: 24rpx;
  color: #00bc71;
}

.goto {
  height: 47rpx;
  width: 100rpx;
  background-color: #00bc71;
  color: #fff;
  font-size: 28rpx;
  line-height: 47rpx;
  text-align: center;
  border-radius: 5rpx;
  position: absolute;
  top: 0rpx;
  right: 0rpx;
}
.search-c {
  margin-top: 20rpx;
  border-top: 1rpx solid #fafafb;
  padding-top: 20rpx;
}
.tishi {
  width: 750rpx;
  text-align: center;
  margin: 0 auto;
  color: #666666;
  margin-top: 50rpx;
}
